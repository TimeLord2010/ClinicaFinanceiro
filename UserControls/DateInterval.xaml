<UserControl x:Class="Financeiro.UserControls.DateInterval"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Financeiro.UserControls" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="800" Background="White">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="30"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox VerticalAlignment="Center" x:Name="BeginMTB"></TextBox>
        <TextBlock Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center">-</TextBlock>
        <TextBox Grid.Column="2" VerticalAlignment="Center" x:Name="EndMTB"></TextBox>
    </Grid>
</UserControl>
